#Makefile

main2: clean main2.o libMyTerm.a libSimpleComputer.a
	gcc -o main2 main2.o -L. -lMyTerm -lSimpleComputer

main2.o: main2.c
	gcc -c main2.c

libMyTerm.a: myTerm.o
	ar rc libMyTerm.a myTerm.o

myTerm.o: myTerm.c
	gcc -c myTerm.c

libSimpleComputer.a: sc.o
	ar rc libSimpleComputer.a sc.o

sc.o: sc.c
	gcc -c sc.c

clean:
	rm -f *.o *.a
